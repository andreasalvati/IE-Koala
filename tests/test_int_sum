import numpy as np
import ie_koala as ko

def test_int_sum():
    my_dict = {
        "int": [1, 2, 3],
        "float": [1.1, 2.2, 3.3],
        "str": ["one", "two", "three"],
        "bool": [True, False, True],
    }

    df = ko.Dataframe(data=my_dict)

    expected_sum = 6
    output = sum(df["int"])

    assert expected_sum == output
